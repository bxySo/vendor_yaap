<?xml version="1.0" encoding="utf-8"?>
<resources>
	<!-- URL base to find .update and .sign files, %s is expanded to property_device value. Path is suffixed by (expanded) filename_base -->
	<string name="url_base_update">https://sourceforge.net/projects/bxyso-android-builds/files/yaap/%s/vanilla/</string>

	<!-- URL base to find the full zip files, %s is replaced to be property_device value. -->
	<string name="url_base_full">https://sourceforge.net/projects/bxyso-android-builds/files/yaap/%s/vanilla/</string>

	<!-- URL base to find the full zip sha256sum files, %s is replaced to be property_device value. -->
	<string name="url_base_full_sum">https://sourceforge.net/projects/bxyso-android-builds/files/yaap/%s/vanilla/</string>

	<!-- URL base suffix - Used only for download -->
	<string name="url_base_suffix">/download</string>

	<!-- the branch name to format URLs with -->
	<string name="url_branch_name">vanilla-signed</string>

	<!-- URL base to fetch all builds. first %s is replaced by branch name later on by property_device -->
	<string name="url_base_json" formatted="false">https://raw.githubusercontent.com/Live-House/ota-info/%s/%s/%s.json</string>

	<!-- github API url, pointing to the OTA json repo. %s formatted as described above -->
	<string name="url_api_history" formatted="false">https://api.github.com/repos/Live-House/ota-info/commits?sha=%s&amp;path=%s/%s.json</string>

	<!-- URL base to fetch cert jsons. first %s is replaced by branch name second by property_device -->
	<string name="url_cert_json" formatted="false">https://raw.githubusercontent.com/Live-House/ota-info/%s/%s/cert.json</string>
</resources>
